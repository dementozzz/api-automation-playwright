{
	"channel": "${{ env.SLACK_CHANNEL_ID }}",
	"blocks": [
		{
			"type": "header",
			"text": {
				"type": "plain_text",
				"text": "Test Result"
			}
		},
		{
			"type": "divider"
		},
		{
			"type": "section",
			"text": {
				"type": "mrkdwn",
				"text": ":white_check_mark:  All tests PASSED! "
			}
		},
		{
			"type": "context",
			"elements": [
				{
					"type": "mrkdwn",
					"text": ":clock3:  ${{ env.SLACK_DURATION }} "
				}
			]
		},
		{
			"type": "section",
			"text": {
				"type": "mrkdwn",
				"text": "*Build number:* ${{ env.SLACK_BUILD_NUMBER }} "
			}
		},
		{
			"type": "divider"
		},
		{
			"type": "context",
			"elements": [
				{
					"type": "mrkdwn",
					"text": ":bust_in_silhouette:  Run by ${{ github.actor }}"
				}
			]
		},
		{
			"type": "actions",
			"elements": [
				{
					"type": "button",
					"text": {
						"type": "plain_text",
						"text": "See Results"
					},
					"value": "button_click",
					"url": "${{ env.GITHUB_URL_BUILD }}"
				}
			]
		}
	]
}