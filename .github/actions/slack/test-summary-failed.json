{
    "channel": "${{ env.SLACK_CHANNEL_ID }}",
    "blocks": [
      {
        "type": "header",
        "text": {
          "type": "plain_text",
          "text": "Test Result"
        }
      },
      {
        "type": "divider"
      },
      {
        "type": "section",
        "text": {
          "type": "mrkdwn",
          "text": ":x:  Some of your test FAILED "
        }
      },
      {
        "type": "context",
        "elements": [
          {
            "type": "mrkdwn",
            "text": ":clock3:  ${{ env.SLACK_DURATION }} "
          }
        ]
      },
      {
        "type": "section",
        "text": {
          "type": "mrkdwn",
          "text": "*Build number:* ${{ env.SLACK_BUILD_NUMBER }} "
        },
        "accessory": {
          "type": "button",
          "text": {
            "type": "plain_text",
            "text": "See results",
            "emoji": true
          },
          "value": "click_me_123",
          "action_id": "button-action"
        }
      },
      {
        "type": "divider"
      },
      {
        "type": "context",
        "elements": [
          {
            "type": "mrkdwn",
            "text": ":bust_in_silhouette:  Run by ${{ github.actor }}"
          }
        ]
      }
    ]
}